{% extends "dashboard.html.twig" %}

{% block body %}

<table class="table">
  <thead>
    <tr>
      <th scope="col">Nom du gite</th>
      <th scope="col">Action</th>
    </tr>
  </thead>
  <tbody>
  {% for gite in gites %}

    <tr>
      
      <td>{{ gite.id }}</td>
      <td>{{ gite.nom }}</td>
      <td>
        <button>Modifier</button>
        <button>Supprimer</button>
      </td>
    </tr>

    {% endfor %}
    
  </tbody>
</table>

{% endblock %}